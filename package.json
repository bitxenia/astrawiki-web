  "devDependencies": {
    "@tailwindcss/typography": "^0.5.14",
    "@types/diff-match-patch": "^1.0.36",
    "@types/next": "^9.0.0",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.6",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.6",
    "eslint-config-prettier": "^10.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.1",
    "postcss": "^8",
    "prettier-plugin-tailwindcss": "^0.6.10",
